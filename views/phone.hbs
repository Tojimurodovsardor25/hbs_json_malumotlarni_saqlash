<h1>This is {{title}}</h1>

<div class="row">
  <div class="col s12 m7">
    <div class="card">
      <div class="card-image">
        <img src="{{img}}" />
        <span class="card-title">{{title}}</span>
      </div>
      <div class="card-content">
        <p>{{price}}</p>
      </div>
      <div class="card-action">
        <a href="/delete/{{id}}">Delete</a>
      </div>
    </div>
  </div>
</div>